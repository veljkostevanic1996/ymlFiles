pipeline{

    agent any

    stages{

        stage('clone'){

            steps{

                git branch: 'master',
                url: 'https://github.com/brki18/devops_web_goat.git',
                credentialsId: '1'
            }
        }

        stage('add permisions'){

            steps{

                sh 'sudo chmod -R 777 /home/vstevanic/Desktop/ymlFilesCopy/'
            }
        }
    }
}